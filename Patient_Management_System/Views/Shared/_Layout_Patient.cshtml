        .slider-two__carousel {
            width: 100%;
            /*max-width: 945px;*/
            position: relative;
            z-index: 19;
        }

            .slider-two__carousel .owl-stage-outer {
                overflow: visible;
            }

            .slider-two__carousel .owl-item {
                opacity: 0;
                visibility: hidden;
                transition: all 500ms ease;
            }

                .slider-two__carousel .owl-item.active {
                    opacity: 1;
                    visibility: visible;
                    padding-right: 30px
                }

        .slider-two__carousel-single img {
            border-radius: 10px;
        }

        .slider-two__carousel-content {
            -webkit-animation-duration: 3s;
            animation-duration: 3s;
            -webkit-animation-fill-mode: both;
            animation-fill-mode: both;
            -webkit-backface-visibility: visible !important;
            backface-visibility: visible !important;
            background: #fff;
            width: 100%;
            max-width: 360px;
            /*position: absolute;
        bottom: -100px;*/
            left: -130px;
            z-index: 10;
            /*background-color: #fff;
        box-shadow: -14.142px 14.142px 30px 0px rgba(67, 131, 99, 0.08);*/
            padding-left: 25px;
            padding-right: 25px;
            padding-top: 25px;
            padding-bottom: 25px;
            border: 1px solid #ccc;
            /*border-top-left-radius: 50px;
        border-bottom-right-radius: 50px;*/
        }

            .slider-two__carousel-content img {
                width: 80px !important;
                margin: 0 auto;
                margin-bottom: 20px;
                display: block
            }

        .orange-font {
            margin: 0 auto
        }

        .slider-two__carousel-content::after {
            content: '';
            width: 3px;
            height: 30px;
            background: linear-gradient(to right, #ed3437, #f89c32);
            position: absolute;
            top: 5%;
            left: 15px;
            transition: all 1s;
        }

        .slider-two__carousel-single:hover .slider-two__carousel-content::after {
            top: 80%;
        }

        .slider-two__carousel-single:hover .slider-two__carousel-content::before {
            top: 10%;
        }

        .slider-two__carousel-content::before {
            content: '';
            width: 3px;
            height: 30px;
            background: linear-gradient(to right, #ed3437, #f89c32);
            position: absolute;
            top: 80%;
            right: 25px;
            transition: all 1s;
        }

        .slider-two__carousel-content h3 {
            margin: 0;
            color: var(--thm-black);
            font-weight: 550;
            font-size: 20px;
            margin-bottom: 20px;
            line-height: 34px;
            text-align: center
        }

        .slider-two__carousel-content p {
            font-size: 16px;
            line-height: 30px;
            color: #93959e;
            margin: 0;
        }

        /* animating on active */

        .slider-two__carousel .active .slider-two__carousle-content {
            animation-name: fadeInUp;
        }
    </style>


